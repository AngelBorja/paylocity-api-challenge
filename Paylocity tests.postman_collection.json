		{
			"name": "Get Employees",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let employees = pm.response.json();\r",
							"let found = false;\r",
							"\r",
							"let ID = pm.collectionVariables.get(\"ID\");\r",
							"\r",
							"employees.forEach(emp => {\r",
							"    if (emp.id === ID) {\r",
							"        found = true;\r",
							"        \r",
							"        pm.test(\"First name is correct\", () => {\r",
							"            pm.expect(emp.firstName, \"did not find the expected first name for the user\").to.eql(pm.collectionVariables.get(\"firstName\"));\r",
							"        });\r",
							"\r",
							"        pm.test(\"Last name is correct\", () => {\r",
							"            pm.expect(emp.lastName, \"did not find the expected last name for the user\").to.eql(pm.collectionVariables.get(\"lastName\"));\r",
							"        });\r",
							"        pm.test(\"Dependants value is correct\", () => {\r",
							"            pm.expect(emp.dependants, \"did not find the expected dependants value for the user\").to.eql(pm.collectionVariables.get(\"dependants\"));\r",
							"        });\r",
							"        pm.test(\"Salary is correct\", () => {\r",
							"            pm.expect(emp.salary, \"did not find the expected salary value for the user\").to.eql(pm.collectionVariables.get(\"salary\"));\r",
							"        });\r",
							"        pm.test(\"Net Salary is correct\", () => {\r",
							"            pm.expect(emp.net, \"did not find the expected last name for the user\").to.eql(pm.collectionVariables.get(\"netsalary\"));\r",
							"        });                        \r",
							"    }\r",
							"});\r",
							"\r",
							"pm.test(\"User exists in employee list\", () => {\r",
							"    pm.expect(found, \"did not find the user ID in the user list\").to.be.true;\r",
							"});\r",
							""
						],
						"type": "text/javascript",
