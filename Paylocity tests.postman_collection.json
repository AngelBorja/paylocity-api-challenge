		{
			"name": "Create Employee",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let response = pm.response.json();\r",
							"let id = response.id;\r",
							"let firstName = response.firstName;\r",
							"let lastName = response.lastName;\r",
							"let dependants = response.dependants;\r",
							"let salary = response.salary;\r",
							"let netsalary = response.net;\r",
							"\r",
							"pm.collectionVariables.set(\"ID\", id);\r",
							"pm.collectionVariables.set(\"firstName\", firstName);\r",
							"pm.collectionVariables.set(\"lastName\", lastName);\r",
							"pm.collectionVariables.set(\"dependants\", dependants);\r",
							"pm.collectionVariables.set(\"salary\", salary);\r",
							"pm.collectionVariables.set(\"netsalary\", netsalary);\r",
							"\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Response body is as expected\", function () { \r",
							"    pm.expect(response).to.have.property(\"id\", id);\r",
							"    pm.expect(response).to.have.property(\"firstName\", firstName);\r",
							"    pm.expect(response).to.have.property(\"lastName\", lastName);\r",
							"    pm.expect(response).to.have.property(\"dependants\", dependants);\r",
							"    pm.expect(response).to.have.property(\"net\", netsalary);\r",
							"});\r",
							"\r",
							"\r",
							""
						],
